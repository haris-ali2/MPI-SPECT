      \strng{namehash}{480592cec4714f1dbd9d26c323be9497}
      \strng{fullhash}{1ff34ace5d0df85927b587fbfa3b2372}
      \strng{bibnamehash}{480592cec4714f1dbd9d26c323be9497}
